{
  "name": "photofield-e2e",
  "version": "1.0.0",
  "description": "Playwright tests for Photofield, see ./ui/ for frontend",
  "main": "vetur.config.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "bddgen": "bddgen",
    "test": "npx bddgen && npx playwright test",
    "watch:bdd": "nodemon -w ./tests -e feature,js,ts --exec npx bddgen",
    "watch:pw": "playwright test",
    "watch": "run-p watch:*",
    "report": "npx playwright show-report",
    "steps": "npx bddgen export"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@types/node": "^22.13.14",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "playwright-bdd": "^8.2.1",
    "typescript": "^5.8.2"
  }
}