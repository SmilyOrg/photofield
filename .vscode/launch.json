{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
    },
    {
      "name": "Debug w/ UI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags embedui"
    },
    {
      "name": "Debug bddgen",
      "request": "launch",
      "program": "${workspaceFolder}/e2e/node_modules/playwright-bdd/dist/cli/index.js",
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Debug gen-photos",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": [
        "-gen-photos",
        "-gen-photos.count",
        "300",
        "-gen-photos.seed",
        "12345",
        "-gen-photos.name",
        "debug-300-30-20",
        "-gen-photos.output",
        "./testdata",
        "-gen-photos.widths",
        "30",
        "-gen-photos.heights",
        "20"
      ]
    },
  ]
}